        -image "commandButton.png" 
        -image1 "commandButton.png" 
        -style "iconOnly" 
        -marginWidth 0
        -marginHeight 1
        -command "/*\n    Script:     aj_UVTransfer.mel\n    Author:    \tAli Asad Jafri\n    Website:   \thttp://www.cgrama.com\n    Name:       AJ - UV Transfer v1.1\n\n    Descr:     \tTransfers UV sets of multiple objects\n                Original code based on Suchan Bajracharya's SRB - UV Transfer\n                Logic changed and some features added to this script to allow a little more flexibility.\n*/\n\n/////////////////////////////////\n//\n//\taj_UVTransfer\n//\n//\tDesc:\tThis tool is used to automatically transfer UV Sets from multiple objects.\n//\t\tTo transfer UV sets load source objects and then target objects. Click apply to transfer\n/////////////////////////////////\n\n// Global Procedure\nglobal proc aj_UVTransfer() {\n\n    // User Interface  \n    if (`window -exists ajUVTransferUI`) deleteUI ajUVTransferUI;\n\n    if (`windowPref -exists ajUVTransferUI`) windowPref -r ajUVTransferUI; \n    \n    window -rtf 1\n           -title \"AJ - UV Transfer v1.1\" -width 410\n           ajUVTransferUI;\n\n    string $about = \"print \\\"AJ - UV Transfer v1.1 || Author: Syed Ali Asad Jafri || xxclusiveasad@gmail.com\\\"\";\n    // Window Menu \n    menuBarLayout;\n\t\t\t\n\tmenu -l \"Edit\";\n\t\tmenuItem -l \"Reset\" -c aj_UVTransferReset;\n\t\n\tmenu -l \"Help\";\n\t\tmenuItem -l \"Help...\" -c aj_UVTransferHelp;\n\t\tmenuItem -divider true;\n\t\tmenuItem -l \"About\" -c $about;\t\n        \n    columnLayout \n        -adjustableColumn true\n        -columnAlign \"center\";\n\n    separator -h 10;\n    text \n        -label \"Note: Polygons both source and target objects must have same topology.\";        \n    separator -h 10;     \n\n    setParent..;\n    setParent..;\n                    \n    columnLayout -adj 1; \n    \n    rowLayout \n        -numberOfColumns 2\n        -columnWidth2 200 200    \n        -columnAttach 1 \"both\" 5\n        -columnAttach 2 \"both\" 5;\n        \n    // source list\n    columnLayout \n        -adjustableColumn true\n        -columnAlign \"center\";\n        \n    text \n        -label \"Source Objects\";\n        \n    textScrollList \n        -numberOfRows 20\n        -allowMultiSelection false\n        ajUVSourceTSL;\n        \n    setParent..;\n    \n    //Target List\n    columnLayout \n        -adjustableColumn true\n        -columnAlign \"center\";\n    \n    text \n        -label \"Target Objects\";\n        \n    textScrollList \n        -numberOfRows 20\n        -allowMultiSelection false\n        ajUVTargetTSL;\n        \n    setParent..;\n    setParent..;    \n    separator -h 10; \n\n        \n    rowLayout \n        -numberOfColumns 2\n        -columnWidth2 200 200\n        -columnAttach 1 \"both\" 5\n        -columnAttach 2 \"both\" 5;\n        \n    radioCollection ajUVTransferRadioCollection; \n        radioButton -sl -label \"Transfer One --> One\" ajUVTransferOneRB;\n        radioButton -label \"Transfer One --> All\" ajUVTransferAllRB;\n      \n    setParent..;\n    separator -h 10;\n    setParent..;\n\n        \n    rowLayout \n        -numberOfColumns 2\n        -columnWidth2 200 200\n        -columnAttach 1 \"both\" 5\n        -columnAttach 2 \"both\" 5;\n        \n    radioCollection ajUVSetRadioCollection; \n        radioButton -sl -label \"Copy to Default UV Set\" ajUVSetDefault;\n        radioButton -label \"Copy to New UV Set\" ajUVSetNew;\n      \n    setParent..;\n    separator -h 10;\n    setParent..;\n    rowLayout \n        -numberOfColumns 4\n        -columnWidth4 100 100 100 100\n        -columnAttach 1 \"both\" 5\n        -columnAttach 2 \"both\" 5\n        -columnAttach 3 \"both\" 5\n        -columnAttach 4 \"both\" 5;\n        \n    button -bgc 0.8 0.8 0.8 -l \"Up\" -c \"ajSelItemUp(\\\"ajUVSourceTSL\\\")\";\n    button -bgc 0.8 0.8 0.8 -l \"Down\" -c \"ajSelItemDn(\\\"ajUVSourceTSL\\\")\";\n  \n    button -bgc 0.8 0.8 0.8 -l \"Up\" -c \"ajSelItemUp(\\\"ajUVTargetTSL\\\")\";\n    button -bgc 0.8 0.8 0.8 -l \"Down\" -c \"ajSelItemDn(\\\"ajUVTargetTSL\\\")\";\n    \n    setParent..;\n\n    rowLayout \n        -numberOfColumns 2\n        -columnWidth2 200 200\n        -columnAttach 1 \"both\" 5\n        -columnAttach 2 \"both\" 5;\n        \n    button -bgc 0.8 0.8 0.8 -l \"Add Source Objects\" -c \"ajUVAddToList(0)\";\n    button -bgc 0.8 0.8 0.8 -l \"Add Target Objects\" -c \"ajUVAddToList(1)\";\n    \n    setParent..;\n\n    rowLayout \n        -numberOfColumns 2\n        -columnWidth2 200 200\n        -columnAttach 1 \"both\" 5\n        -columnAttach 2 \"both\" 5;\n        \n    button -bgc 0.5 0.5 0.5 -l \"Remove All Source Objects\" -c (\"textScrollList -edit -removeAll ajUVSourceTSL\");\n    button -bgc 0.5 0.5 0.5 -l \"Remove All Target Objects\" -c (\"textScrollList -edit -removeAll ajUVTargetTSL\");\n    \n    //Set Parent to the Main Layout\n    setParent..; \n    button -l \"Apply\" -c \"ajUVTransferProc\";    \n\n        \n    showWindow ajUVTransferUI;\n    \n}\n\n// Add to list procedure\nglobal proc ajUVAddToList(int $choice)\n{\n    string $ajSelObjs[] = `ls -sl`;    \n    \n    if($choice == 0)\n    {\n        for ($objs in $ajSelObjs) \n        {\n            textScrollList -edit -append $objs ajUVSourceTSL;\n        }\n    }\n    else\n    {\n        for ($objs in $ajSelObjs)\n        {\n            textScrollList -edit -append $objs ajUVTargetTSL;        \n        }\n    }    \n\n}\n\n// Procedure for transfer\nglobal proc ajUVTransferProc() {\n\n// Variable Declaration\n    int $ajUVTransferOneOptionVar = `radioButton -q -sl ajUVTransferOneRB`;\n\tint $ajUVTransferAllOptionVar = `radioButton -q -sl ajUVTransferAllRB`;\n\t\n\tint $ajUVSetDefault = `radioButton -q -sl ajUVSetDefault`;\n\tint $ajUVSetNew = `radioButton -q -sl ajUVSetNew`;\n\t\n\tstring $ajSourceObjectsList[] = `textScrollList -q -ai ajUVSourceTSL`;\n\tstring $ajTargetObjectsList[] = `textScrollList -q -ai ajUVTargetTSL`;\t\n\t\n\tint $ajSizeSourceObjs = `size $ajSourceObjectsList`;\n\tint $ajSizeTargetObjs = `size $ajTargetObjectsList`;\n\t\n    // string $ajNewUVTargetSets[];\n\t\n// For One --> To All. UV Transfer.\n\n// If Transfer set to Default UV Set\n    if ($ajUVSetDefault == 1) \n    {\n        if ($ajUVTransferAllOptionVar == 1) \n        {\n            if($ajSizeSourceObjs == 1)\n            {\n                for ($objs in $ajTargetObjectsList)\n                {\n                    polyTransfer -v 0 -vc 0 -uv 1 -ao $ajSourceObjectsList[0] $objs;         \n                }      \n                print \"AJ - UV Transfer v1.1 has sucessfully transferred the UV...\\n\";  \n            }\n\t        else\n    \t    {\n        \t    warning \"AJ - UV Transfer v1.1 has encounterred a problem. Source object are great than target objects...terminating...\\n\";\n        \t}\t\n        }\n    }\n    \n// For One --> To All. UV Transfer.\n    \n// If Transfer set to New UV Set\n    if ($ajUVSetNew == 1) \n    {\n        if ($ajUVTransferAllOptionVar == 1) \n        {\n            if($ajSizeSourceObjs == 1)\n            {\n                for ($objs in $ajTargetObjectsList)\n                {\n                   // polyTransfer -v 0 -vc 0 -uv 1 -ao $ajSourceObjectsList[0] $objs;   \n                   string $ajPolyTarget = $objs;      \n                   polyUVSet -create -uvSet \"NewUVSet\" $ajPolyTarget;\n                   polyUVSet -currentLastUVSet;\n\n                   // $ajNewUVTargetSets = `polyUVSet -q -currentUVSet $ajPolyTarget`;\n\n                   transferAttributes -sampleSpace 4 -transferUVs 1 -transferColors 0 -sourceUvSet \"map1\" -targetUvSet \"NewUVSet\" $ajSourceObjectsList[0] $ajPolyTarget;\n                }      \n                print \"AJ - UV Transfer v1.1 has sucessfully transferred the UV...\\n\";  \n            }\n\t        else\n    \t    {\n        \t    warning \"AJ - UV Transfer v1.1 has encounterred a problem. Source object are great than target objects...terminating...\\n\";\n        \t}\t\n        }\n    }\n   \n    \n// For One --> to One. UV Transfer.   \n\n// If Transfer set to Default UV Set \n    if ($ajUVSetDefault == 1) \n    {\n        if ($ajUVTransferOneOptionVar == 1)\n        {\n            if ($ajSizeSourceObjs == $ajSizeTargetObjs)\n            {\n                for($i=0; $i<$ajSizeSourceObjs; $i++)\n                {\n\t    \t\t\tpolyTransfer -v 0 -vc 0 -uv 1 -ao $ajSourceObjectsList[$i] $ajTargetObjectsList[$i];\n                }\n                print \"AJ - UV Transfer v1.1 has sucessfully transferred the UV...\\n\";  \n            }\n            else \n                {\n      \t\t    warning \"AJ - UV Transfer v1.1 has encounterred a problem. Source object is not equal to target objects...terminating...\\n\";        \n                }\n         } \n     }\n    \n// For One --> To One. UV Transfer.\n    \n// If Transfer set to New UV Set\n\n    if ($ajUVSetNew == 1) \n    {\n        if ($ajUVTransferOneOptionVar == 1)\n        {\n            if ($ajSizeSourceObjs == $ajSizeTargetObjs)\n            {\n                for($i=0; $i<$ajSizeSourceObjs; $i++)\n                {\n\t    \t\t// polyTransfer -v 0 -vc 0 -uv 1 -ao $ajSourceObjectsList[$i] $ajTargetObjectsList[$i];\n                   string $ajPolyTarget = $ajTargetObjectsList[$i];;\n                   polyUVSet -create -uvSet \"NewUVSet\" $ajTargetObjectsList[$i];\n                   polyUVSet -currentLastUVSet;\n\n                   // $ajNewUVTargetSets = `polyUVSet -q -currentUVSet $ajPolyTarget`;\n\n                   transferAttributes -sampleSpace 4 -transferUVs 1 -transferColors 0 -sourceUvSet \"map1\" -targetUvSet \"NewUVSet\" $ajSourceObjectsList[$i] $ajPolyTarget;\n                }\n                print \"AJ - UV Transfer v1.1 has sucessfully transferred the UV...\\n\";  \n            }\n            else \n                {\n      \t\t    warning \"AJ - UV Transfer v1.1 has encounterred a problem. Source object is not equal to target objects...terminating...\\n\";        \n                }\n            } \n     }\n\n}\n\n// Help Window.\nglobal proc aj_UVTransferHelp()\n{\n    if (`window -exists aj_UVTransferHelpUI`) deleteUI aj_UVTransferHelpUI;\n\n    if (`windowPref -exists aj_UVTransferHelpUI`) windowPref -r aj_UVTransferHelpUI; \n\n    window -mxb 0\n\t        -wh 445 379\n\t        -title \"AJ - UV Transfer v1.1 Help\" \n        \taj_UVTransferHelpUI;\n\n    $ajWindowCollapseCmd = \"window -e -wh 445 20 aj_UVTransferHelpUI\";\n    $ajWindowExpandCmd = \"window -e -wh 445 379 aj_UVTransferHelpUI\";\n\n    frameLayout -l \"Help\"\n                -collapsable on\n                // -collapse on\n                -borderStyle \"etchedIn\"\n                -marginHeight 2\n                -marginWidth 2\n                -cc $ajWindowCollapseCmd\n                -ec $ajWindowExpandCmd\n                helpFrame;\n\n    columnLayout mainColumn;\n    string $helpContent;\n\n    $helpContent = (\"This script eases the pain of transfering UV's from a list of objects either One to All, or One to One in List.\"\n                    + \"\\n\\n\"\n                    + \"Polygon objects must have same topology for UV to transfer properly.\\n\"           \n                    + \"\\n\\n\"\n                    + \"===========================================================\"\n                    + \"\\n\"\n                    + \"Update v1.1\"\n                    + \"\\n\"\n                    + \"===========================================================\"\n                    + \"\\n\"\n                    + \"Polygon UV transfer can done on Default UV Set or allow creating a new UV Set by checking Copy to New UV Set\"\n                    + \"\\n\"                    \n                    + \"\\n\"\n                    +\"For suggestions or comments you can email at xxclusiveasad@gmail.com\");\n\n    scrollField -wordWrap true -ed off -w 435 -h 350 -text $helpContent helpScroll;\n    setParent mainColumn;\n\n\n    showWindow aj_UVTransferHelpUI;\n\n}\n\n// Reset UV Transfer utility.\nglobal proc aj_UVTransferReset() {\n    textScrollList -edit -removeAll ajUVSourceTSL;\n    textScrollList -edit -removeAll ajUVTargetTSL;\n    radioCollection -edit -select ajUVTransferOneRB ajUVTransferRadioCollection;\n    radioCollection -edit -select ajUVSetDefault ajUVSetRadioCollection;     \n}\n\n\n\nglobal proc ajSelItemUp(string $ajTSLName)\n{\n    int $ajSelItemIndex[] = `textScrollList -q -sii $ajTSLName`;\n\n    if (size($ajSelItemIndex) == 0)\n    {\n        error \"No Items Selected!\";\n    }\n\n    int $ajItemIndex = $ajSelItemIndex[0];\n\n    $ajItemIndex--;\n\n    if ($ajItemIndex > 0) {\n        string $ajSelItemIndexContent[] = `textScrollList -q -si $ajTSLName`;\n        string $ajItemContent = $ajSelItemIndexContent[0];\n        textScrollList -e -rii $ajSelItemIndex[0] $ajTSLName;\n        textScrollList -e -ap $ajItemIndex $ajItemContent $ajTSLName;\n        textScrollList -e -si $ajItemContent $ajTSLName;\n    }\n}\n\nglobal proc ajSelItemDn(string $ajTSLName)\n{\n    int $ajSelItemIndex[] = `textScrollList -q -sii $ajTSLName`;\n\n    if (size($ajSelItemIndex) == 0)\n    {\n        error \"No Items Selected!\";\n    }\n\n    int $ajItemIndex = $ajSelItemIndex[0];\n    $ajItemIndex++;\n    if ($ajItemIndex <= `textScrollList -q -ni $ajTSLName`){\n        string $ajSelItemIndexContent[] = `textScrollList -q -si $ajTSLName`;\n        string $ajItemContent = $ajSelItemIndexContent[0];\n        textScrollList -e -rii $ajSelItemIndex[0] $ajTSLName;\n        textScrollList -e -ap $ajItemIndex $ajItemContent $ajTSLName;\n        textScrollList -e -si $ajItemContent $ajTSLName;\n    }\n}\n\naj_UVTransfer();\n\n\n\n\n" 
