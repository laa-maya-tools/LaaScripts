        -enableBackground 0
        -backgroundColor 0 0 0 
        -highlightColor 0.321569 0.521569 0.65098 
        -align "center" 
        -label "/*\n    Script:     aj_UVTransfer.mel\n    Author:    \tAli Asad Ja..." 
        -labelOffset 0
        -rotation 0
        -flipX 0
        -flipY 0
        -useAlpha 1
        -font "plainLabelFont" 
        -imageOverlayLabel "TUVs" 
        -overlayLabelColor 0.8 0.8 0.8 
