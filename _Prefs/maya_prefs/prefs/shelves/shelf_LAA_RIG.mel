        -overlayLabelColor 0.8 0.8 0.8 
        -overlayLabelBackColor 0 0 0 0.2 
        -image "C:/Users/leandro_laa/Documents/maya/2015-x64/prefs/icons/laa_zeroJointOrientation.png" 
